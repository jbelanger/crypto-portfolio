{
  "name": "crypto-tx-import",
  "version": "1.0.0",
  "description": "Crypto transaction import and verification tool using CCXT - Monorepo",
  "private": true,
  "scripts": {
    "build": "pnpm -F crypto-portfolio-cli build",
    "clean": "pnpm -r run clean",
    "dev": "pnpm -F crypto-portfolio-cli dev",
    "import": "pnpm -F crypto-portfolio-cli import",
    "import:verify": "pnpm -F crypto-portfolio-cli import:verify",
    "verify": "pnpm -F crypto-portfolio-cli verify",
    "status": "pnpm -F crypto-portfolio-cli status",
    "export": "pnpm -F crypto-portfolio-cli export",
    "test": "pnpm -F crypto-portfolio-cli test",
    "test:unit": "pnpm -r test",
    "test:e2e": "pnpm -F crypto-portfolio-cli test --exchange",
    "test:watch": "pnpm -r run test --watch",
    "test:coverage": "pnpm -r run test --coverage",
    "test:coinbase": "pnpm -F crypto-portfolio-cli test --exchange coinbase",
    "test:coinbase:e2e": "pnpm -F crypto-portfolio-cli test --exchange coinbase",
    "typecheck": "pnpm -r run typecheck",
    "lint": "pnpm -r run lint",
    "prettier": "pnpm -r run prettier",
    "prettier:fix": "pnpm -r run prettier:fix",
    "providers:list": "pnpm -F @crypto/import providers:list",
    "providers:validate": "pnpm -F @crypto/import providers:validate",
    "config:generate": "pnpm -F @crypto/import config:generate",
    "config:validate": "pnpm -F @crypto/import config:validate",
    "workspace:build": "pnpm -r build",
    "workspace:clean": "pnpm -r clean",
    "workspace:test": "pnpm -r test"
  },
  "keywords": [
    "ccxt",
    "crypto",
    "transactions",
    "import",
    "balance-verification"
  ],
  "engines": {
    "node": ">=23.0.0",
    "pnpm": ">=10.6.2"
  },
  "packageManager": "pnpm@10.6.2"
}