{
  "bitcoin": {
    "explorers": [
      {
        "name": "mempool.space",
        "displayName": "Mempool.space API (Custom)",
        "enabled": true,
        "priority": 1,
        "type": "rest",
        "description": "Custom override for Mempool.space with higher priority",
        "capabilities": {
          "supportedOperations": ["getRawAddressTransactions", "getAddressInfo"],
          "maxBatchSize": 25,
          "supportsHistoricalData": true,
          "supportsRealTimeData": true,
          "supportsTokenData": false,
          "supportsPagination": true
        },
        "networks": {
          "mainnet": {
            "baseUrl": "https://mempool.space/api"
          },
          "testnet": {
            "baseUrl": "https://mempool.space/testnet/api"
          }
        },
        "defaultConfig": {
          "timeout": 5000,
          "retries": 5,
          "rateLimit": {
            "requestsPerSecond": 0.5,
            "requestsPerMinute": 30,
            "requestsPerHour": 1000,
            "burstLimit": 2
          }
        }
      },
      {
        "name": "tatum",
        "displayName": "Tatum Bitcoin API (Override)",
        "enabled": false,
        "priority": 10,
        "type": "rest",
        "requiresApiKey": true,
        "apiKeyEnvVar": "TATUM_API_KEY",
        "description": "Tatum provider disabled via config override",
        "capabilities": {
          "supportedOperations": ["getRawAddressTransactions", "getAddressInfo"],
          "maxBatchSize": 50,
          "supportsHistoricalData": true,
          "supportsRealTimeData": true,
          "supportsTokenData": false,
          "supportsPagination": true
        },
        "networks": {
          "mainnet": {
            "baseUrl": "https://api.tatum.io/v3/bitcoin"
          },
          "testnet": {
            "baseUrl": "https://api.tatum.io/v3/bitcoin-testnet"
          }
        },
        "defaultConfig": {
          "timeout": 20000,
          "retries": 2,
          "rateLimit": {
            "requestsPerSecond": 1,
            "requestsPerMinute": 60,
            "requestsPerHour": 3600,
            "burstLimit": 3
          }
        }
      }
    ]
  }
}
