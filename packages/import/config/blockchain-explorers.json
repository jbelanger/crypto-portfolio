{
  "bitcoin": {
    "defaultEnabled": ["mempool.space", "blockstream.info", "blockchain.com"],
    "overrides": {
      "mempool.space": {
        "priority": 1,
        "rateLimit": {
          "requestsPerSecond": 0.5,
          "requestsPerMinute": 30,
          "requestsPerHour": 1000,
          "burstLimit": 2
        }
      },
      "tatum": {
        "enabled": false,
        "priority": 10,
        "description": "Tatum provider disabled via config override"
      },
      "blockcypher": {
        "priority": 5
      }
    }
  },
  "ethereum": {
    "defaultEnabled": ["alchemy", "moralis", "evm-rpc"],
    "overrides": {
      "alchemy": {
        "priority": 1
      },
      "evm-rpc": {
        "priority": 10,
        "description": "Free public RPC endpoint - lower priority fallback"
      }
    }
  },
  "solana": {
    "defaultEnabled": ["solana-rpc", "solscan"],
    "overrides": {
      "helius": {
        "priority": 1
      }
    }
  },
  "avalanche": {
    "defaultEnabled": ["snowtrace"]
  },
  "injective": {
    "defaultEnabled": ["injective-explorer", "injective-lcd"]
  },
  "bittensor": {
    "defaultEnabled": ["taostats"]
  },
  "polkadot": {
    "defaultEnabled": ["subscan"]
  },
  "theta": {
    "defaultEnabled": ["thetascan"]
  }
}
