    "jsonwebtoken": "^9.0.2",
    "neverthrow": "^8.2.0",
    "reflect-metadata": "^0.2.2"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "tsconfig": "workspace:*",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2"
  },
  "peerDependencies": {
    "@exitbook/core": "workspace:*",
    "@exitbook/data": "workspace:*",
    "@exitbook/platform-http": "workspace:*",
    "@exitbook/shared-logger": "workspace:*",
    "@exitbook/shared-utils": "workspace:*"
  }
