{
  "name": "@exitbook/platform-event-store",
  "version": "0.0.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./port": {
      "types": "./dist/port.d.ts",
      "default": "./dist/port.js"
    },
    "./model": {
      "types": "./dist/model.d.ts",
      "default": "./dist/model.js"
    },
    "./registry": {
      "types": "./dist/registry.d.ts",
      "default": "./dist/registry.js"
    },
    "./compose/*": {
      "types": "./dist/compose/*.d.ts",
      "default": "./dist/compose/*.js"
    }
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -b",
    "clean": "rimraf dist",
    "dev": "tsc -b -w",
    "migrate": "node --import=tsx ./src/migrate.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@exitbook/core": "workspace:*",
    "@exitbook/platform-database": "workspace:*",
    "@exitbook/platform-messaging": "workspace:*",
    "bignumber.js": "^9.1.0",
    "effect": "^3.0.0"
  },
  "devDependencies": {
    "@internal/tsconfig": "workspace:*",
    "@types/node": "^24.3.1",
    "kysely": "^0.28.5",
    "rimraf": "^5.0.0",
    "tsx": "^4.0.0"
  }
}
