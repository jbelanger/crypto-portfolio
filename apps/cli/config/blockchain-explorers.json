          "mainnet": {
            "baseUrl": "https://api.blockcypher.com/v1/btc/main"
          },
          "testnet": {
            "baseUrl": "https://api.blockcypher.com/v1/btc/test3"
          }
        },
        "defaultConfig": {
          "timeout": 15000,
          "retries": 3,
          "rateLimit": {
            "requestsPerSecond": 3,
            "requestsPerMinute": 180,
            "requestsPerHour": 10800,
            "burstLimit": 5
          }
        }
      },
